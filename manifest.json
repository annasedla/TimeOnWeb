{
  "manifest_version": 2,

  "name": "Time On Web",
  "description": "This extension that shows how much time do you spent on a website.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
